<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            pageTitle="haag-4"
            jcr:primaryType="cq:PageContent"
            jcr:title="nl"
            cq:contextHubPath="/etc/cloudsettings/default/contexthub"
            cq:contextHubSegmentsPath="/etc/segmentation/contexthub"
            cq:template="/conf/haag-4/settings/wcm/templates/page-content"
            sling:resourceType="haag-4/components/page">
        <root
                jcr:primaryType="nt:unstructured"
                sling:resourceType="haag-4/components/container"
                layout="responsiveGrid">
            <container
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="haag-4/components/container">
                <container
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="haag-4/components/container"
                        layout="responsiveGrid">
                    <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="haag-4/components/text"
                            text="Welcome @ Haag 4!"/>
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
